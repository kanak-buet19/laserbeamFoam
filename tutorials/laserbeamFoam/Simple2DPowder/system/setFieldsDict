FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 0
);

regions
(



    boxToCell
    {
        box (-0.085 0.001 -0.05) (0.085 0.003 0.05);
        fieldValues
        (
    volScalarFieldValue alpha.metal 1
        );
    }


cylinderToCell
{
p1 	 (-1 	 0.0005 	 1e-3);
p2 	 (51 	 0.0005 	 1e-3);
radius 5e-4;
fieldValues
(
            volScalarFieldValue alpha.metal 1
);
}

